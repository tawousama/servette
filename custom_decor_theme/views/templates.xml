                </t>&ndash;&gt;
        </template>-->


        <template id="modal_demandeInfo">
            <style>
                .form-control {
                    color: black!important;
                    background-color: white!important;
                }
            </style>
            <div class="modal fade"  style="max-width:100%; " id="demandeInfoModal" tabindex="-1" role="dialog" aria-labelledby="demandeInfoModalLabel" aria-hidden="true">
                <div class="modal-dialog"  style="max-width:100%;" role="document">
                  <div class="modal-content" style="max-width:100%; background-color: black!important; color:white;">
                    <div id="loadingdiv" style="
                                            position: absolute;
                                            top: 0;
                                            left: 0;
                                            right: 0;
                                            bottom: 0;
                                            z-index:99;
                                            background: #000000c4;
                                            display: none;
                                            justify-content: center;
                                            align-items: center;
                                ">
                                <div id="loading">
                                    <img  src="/web/static/img/spin.png"  class="m-2 fa-pulse"/>
                                    <span style="font-size: 20px">Envoi du formulaire...</span>
                                </div>
                                <div id="loadingsuccess" style="display:none">
                                    <span style="font-size: 20px">Envoi réussi, nous vous contacterons dans les plus bref delais</span>
                                </div>
                            </div>
                    <div class="modal-header" style="max-width:100%">
                                    <h4 class="modal-title" id="send_guest_formLabel">Envoyer une question sur ce produit</h4>
                    </div>
                    <div class="modal-body"  style="max-width:100%" id="send-guest-form-body">
                                       
                                        <!-- <div id="send_error_formproduct"></div> -->
                                            <!-- <span class="required">* Champs requis.</span> -->
                            
                                <div id="demandeInfoForm"  >
                                    <input type="hidden" id="product_template_id" name="product_template_id" />
                                    <div class="form-outline">
                                        <label class="form-label" for="product_template_name">Produit *</label>
                                        <input class="form-control texte" id="product_template_name"  readonly="readonly"/>
                                   </div>
                                    <!-- Name input -->
                                    <div  class="form-outline">
                                        <label class="form-label" for="name2">Votre Nom *</label>
                                        <input type="text" id="name2" name="Votre Nom" class="form-control" required="required" />
                                    </div>
                
